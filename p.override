name: dynamic-providers
desc: 动态订阅

script-providers:
  dynamic-providers:
    url: https://raw.githubusercontent.com/kaeltheron/MyStash/main/dynamic-providers.js
    interval: 86400
rule-providers:
  anti-AD:
    type: http
    behavior: domain
    url: "https://anti-ad.net/clash.yaml"
    path: ./ruleset/anti-ad-clash.yaml
    interval: 86400

rules:
  - RULE-SET,anti-AD,REJECT